{
    "compilerOptions": {
        "module": "ES6",
        "target": "ES2022",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "importHelpers": true,
    },
    "exclude": [
        "node_modules",
        "**/*.d.ts",
        "webpack.config.js"
    ]
}
